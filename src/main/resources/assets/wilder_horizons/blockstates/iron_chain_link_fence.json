{
  "multipart": [
    {
      "apply": {
        "model": "wilder_horizons:block/link_post_ends"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "wilder_horizons:block/link_post"
      }
    },
    {
      "when": {
        "north": "true",
        "west": "false",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "wilder_horizons:block/link_cap"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "false",
        "east": "true"
      },
      "apply": {
        "model": "wilder_horizons:block/link_cap",
        "y": 90
      }
    },
    {
      "when": {
        "north": "false",
        "west": "false",
        "south": "true",
        "east": "false"
      },
      "apply": {
        "model": "wilder_horizons:block/link_cap_alt"
      }
    },
    {
      "when": {
        "north": "false",
        "west": "true",
        "south": "false",
        "east": "false"
      },
      "apply": {
        "model": "wilder_horizons:block/link_cap_alt",
        "y": 90
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "wilder_horizons:block/link_side"
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "wilder_horizons:block/link_side",
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "wilder_horizons:block/link_side_alt"
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "wilder_horizons:block/link_side_alt",
        "y": 90
      }
    }
  ]
}